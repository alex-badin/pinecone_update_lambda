# Telegram API credentials
# Get these from https://my.telegram.org
TG_API_ID=your_telegram_api_id
TG_API_HASH=your_telegram_api_hash
TG_SESSION_STRING=your_telegram_session_string

# AI Service API keys
COHERE_KEY=your_cohere_api_key
PINE_KEY=your_pinecone_api_key
GEMINI_API_KEY=your_gemini_api_key

# Pinecone configuration
PINE_INDEX=your_pinecone_index_name

# Processing configuration (optional)
DAYS_TO_PARSE=2
DB_BATCH_SIZE=1000

# Airtable integration (optional)
AIRTABLE_API_TOKEN=your_airtable_token
AIRTABLE_BASE_ID=your_airtable_base_id
AIRTABLE_TABLE_NAME=your_airtable_table_name

# Database configuration (optional, for PostgreSQL support)
PG_HOST=localhost
PG_PORT=5432
PG_USER=postgres
PG_PASSWORD=your_postgres_password
PG_DATABASE=telegram_data
